{
  "name": "Boilerplate",
  "main": "main.min.js",
  "version": "0.0.1",
  "description": "React boilerplate",
  "scripts": {
    "test:unit": "node_modules/.bin/jest",
    "test:unit:coverage": "node_modules/.bin/jest --coverage",
    "lint": "node_modules/.bin/eslint \"./**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development node ./scripts/start.js 8000",
    "start:prod": "cross-env NODE_ENV=production node ./scripts/start.js 8000",
    "build:dev": "cross-env NODE_ENV=development node ./scripts/build.js",
    "build:prod": "cross-env NODE_ENV=production node ./scripts/build.js",
    "setup:refresh": "npm prune && npm install && npm update"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "react-bootstrap": "^0.30.7",
    "react-redux": "^5.0.2",
